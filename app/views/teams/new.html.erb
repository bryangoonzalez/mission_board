<section class="section">
  <%= content_tag :div,
        id: "team-form",
        date: {
          id: @team.id,
          team: @team.to_json(except: [:id, :created_at, :updated_at]),
          user_attributes: @team.users.to_json(except: [:created_at, :updated_at])
        } do %>
<div class="column is-5">
  <h1 class="subtitle is-2">Create a new team</h1>
  <%= render 'form', team: @team %>
</div>
<div class="column is-5">
  <h2 class="subtitle is-3">Team Name</h2>
  <table class="table is-fullwidth">
    <thead>
      <th>Name</th>
      <th>Email</th>
    </thead>
    <tr>
      <td>User Name</td>
      <td>User Email</td>
    </tr>
  </table>
</div>
<% end %>
</section>
